class Dog:
    def __init__(self, name, breed):
        self.name = name
        self.breed = breed
    
    def bark(self):
        print(f"{self.name} says woof!")
    
    def get_breed(self):
        return self.breed
    
def main():
    dog1 = Dog("Fido", "Labrador")
    dog2 = Dog("Buddy", "Poodle")
    
    dog1.bark()
    dog2.bark()
    
    print(f"{dog1.name} is a {dog1.get_breed()}")
    print(f"{dog2.name} is a {dog2.get_breed()}")
    
if __name__ == "__main__":
    main()
